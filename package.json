{
    "name": "bracket-lens",
    "displayName": "Bracket Lens",
    "description": "Show bracket header on closing bracket.",
    "icon": "images/bracketlenshex.128.png",
    "version": "1.4.3",
    "publisher": "wraith13",
    "license": "SEE LICENSE IN LICENSE_1_0.txt",
    "engines": {
        "vscode": "^1.50.0"
    },
    "bugs": {
        "url": "https://github.com/wraith13/bracket-lens-vscode/issues"
    },
    "homepage": "https://github.com/wraith13/bracket-lens-vscode",
    "repository": {
        "type": "git",
        "url": "https://github.com/wraith13/bracket-lens-vscode.git"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "bracket",
        "code",
        "lens"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/source/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "bracketLens.toggleMute",
                "title": "%bracketLens.toggleMute.title%",
                "category": "Bracket Lens"
            },
            {
                "command": "bracketLens.toggleMuteAll",
                "title": "%bracketLens.toggleMuteAll.title%",
                "category": "Bracket Lens"
            },
            {
                "command": "bracketLens.reportProfile",
                "title": "%bracketLens.reportProfile.title%",
                "category": "Bracket Lens"
            }
        ],
        "configuration": [
            {
                "title": "Bracket Lens",
                "properties": {
                    "bracketLens.enabled": {
                        "type": "boolean",
                        "default": true,
                        "description": "%bracketLens.enabled.description%",
                        "scope": "language-overridable"
                    },
                    "bracketLens.debug": {
                        "type": "boolean",
                        "default": false,
                        "description": "%bracketLens.debug.description%",
                        "scope": "window"
                    },
                    "bracketLens.color": {
                        "type": "string",
                        "default": "#8888AAAA",
                        "$ref": "vscode://schemas/textmate-colors#definitions/colorGroup",
                        "description": "%bracketLens.color.description%",
                        "scope": "language-overridable"
                    },
                    "bracketLens.prefix": {
                        "type": "string",
                        "default": " <- ",
                        "description": "%bracketLens.prefix.description%",
                        "scope": "language-overridable"
                    },
                    "bracketLens.unmatchBracketsPrefix": {
                        "type": "string",
                        "default": " ðŸš« <- ",
                        "description": "%bracketLens.unmatchBracketsPrefix.description%",
                        "scope": "language-overridable"
                    },
                    "bracketLens.maxBracketHeaderLength": {
                        "type": "integer",
                        "default": 64,
                        "minimum": 8,
                        "maximum": 256,
                        "description": "%bracketLens.maxBracketHeaderLength.description%",
                        "scope": "language-overridable"
                    },
                    "bracketLens.minBracketScopeLines": {
                        "type": "integer",
                        "default": 5,
                        "minimum": 2,
                        "maximum": 80,
                        "description": "%bracketLens.minBracketScopeLines.description%",
                        "scope": "language-overridable"
                    },
                    "bracketLens.languageConfiguration": {
                        "type": "object",
                        "properties": {
                            "ignoreCase": {
                                "type": "boolean",
                                "default": false
                            },
                            "comments": {
                                "type": "object",
                                "properties": {
                                    "block": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "required": [
                                                "opening",
                                                "closing"
                                            ],
                                            "properties": {
                                                "opening": {
                                                    "type": "string"
                                                },
                                                "closing": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    },
                                    "line": {
                                        "type": "array",
                                        "items": {
                                            "type": "string"
                                        }
                                    }
                                }
                            },
                            "brackets": {
                                "type": "object",
                                "properties": {
                                    "symbol": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "required": [
                                                "opening",
                                                "closing"
                                            ],
                                            "properties": {
                                                "opening": {
                                                    "type": "string"
                                                },
                                                "closing": {
                                                    "type": "string"
                                                },
                                                "headerMode": {
                                                    "type": "string",
                                                    "default": "smart",
                                                    "enum": [
                                                        "before",
                                                        "smart",
                                                        "inner"
                                                    ]
                                                },
                                                "inters": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "word": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "required": [
                                                "opening",
                                                "closing"
                                            ],
                                            "properties": {
                                                "opening": {
                                                    "type": "string"
                                                },
                                                "closing": {
                                                    "type": "string",
                                                    "default": "end"
                                                },
                                                "headerMode": {
                                                    "type": "string",
                                                    "default": "inner",
                                                    "enum": [
                                                        "before",
                                                        "smart",
                                                        "inner"
                                                    ]
                                                },
                                                "inters": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "strings": {
                                "type": "object",
                                "properties": {
                                    "inline": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "required": [
                                                "opening",
                                                "closing",
                                                "escape"
                                            ],
                                            "properties": {
                                                "opening": {
                                                    "type": "string"
                                                },
                                                "closing": {
                                                    "type": "string"
                                                },
                                                "escape": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    },
                                    "multiline": {
                                        "type": "array",
                                        "items": {
                                            "type": "object",
                                            "required": [
                                                "opening",
                                                "closing",
                                                "escape"
                                            ],
                                            "properties": {
                                                "opening": {
                                                    "type": "string"
                                                },
                                                "closing": {
                                                    "type": "string"
                                                },
                                                "escape": {
                                                    "type": "string"
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "terminators": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "ignoreSymbols": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        },
                        "default": {
                            "ignoreCase": false,
                            "comments": {
                                "block": [
                                    {
                                        "opening": "/*",
                                        "closing": "*/"
                                    }
                                ],
                                "line": [
                                    "//"
                                ]
                            },
                            "brackets": {
                                "symbol": [
                                    {
                                        "opening": "(",
                                        "closing": ")",
                                        "headerMode": "smart",
                                        "inters": []
                                    },
                                    {
                                        "opening": "[",
                                        "closing": "]",
                                        "headerMode": "smart",
                                        "inters": []
                                    },
                                    {
                                        "opening": "{",
                                        "closing": "}",
                                        "headerMode": "smart",
                                        "inters": []
                                    }
                                ],
                                "word": []
                            },
                            "strings": {
                                "inline": [
                                    {
                                        "opening": "\"",
                                        "closing": "\"",
                                        "escape": "\\\""
                                    },
                                    {
                                        "opening": "'",
                                        "closing": "'",
                                        "escape": "\\'"
                                    }
                                ],
                                "multiline": [
                                    {
                                        "opening": "`",
                                        "closing": "`",
                                        "escape": "\\`"
                                    }
                                ]
                            },
                            "terminators": [
                                ";",
                                ","
                            ],
                            "ignoreSymbols": [
                                "\\//"
                            ]
                        },
                        "description": "%bracketLens.languageConfiguration.description%",
                        "scope": "language-overridable"
                    }
                }
            }
        ],
        "configurationDefaults": {
            "[plaintext]": {
                "bracketLens.enabled": false
            },
            "[csv]": {
                "bracketLens.enabled": false
            },
            "[tsv]": {
                "bracketLens.enabled": false
            },
            "[markdown]": {
                "bracketLens.enabled": false
            },
            "[html]": {
                "bracketLens.enabled": false
            },
            "[python]": {
                "bracketLens.enabled": false
            },
            "[xml]": {
                "bracketLens.enabled": false
            },
            "[xsl]": {
                "bracketLens.enabled": false
            },
            "[ignore]": {
                "bracketLens.enabled": false
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint source --ext ts",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "@wraith13/vscel": "^0.0.28"
    },
    "devDependencies": {
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.50.0",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "eslint": "^7.9.0",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    }
}
